{
    "name": "phauthentic/phpstan-rules",
    "type": "library",
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "Phauthentic\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phauthentic\\PHPStanRules\\Tests\\": "tests/",
            "App\\": "data/"
        }
    },
    "authors": [
        {
            "name": "Florian Kr√§mer"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "test-coverage": [
            "phpunit --coverage-text"
        ],
        "test-coverage-html": [
            "phpunit --coverage-html tmp/coverage/"
        ]
    }
}
